# Auto detect text files and perform LF normalization
* text=auto

# Python files
*.py text eol=lf
*.pyx text eol=lf
*.pxd text eol=lf

# Jupyter notebooks
*.ipynb text eol=lf

# Documentation
*.md text eol=lf
*.txt text eol=lf

# CSV files
*.csv text eol=lf

# Binary files
*.pkl binary
*.cbm binary
*.h5 binary
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
